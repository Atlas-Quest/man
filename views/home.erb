<header>
  <h1>
    Page Heading
  </h1>
</header>
<div class="">
  <h2>
    <%= session[:user_name] %>
  </h2>
</div>
<div class="">
  <h2>
    <%= @message %>
  </h2>
</div>
<div class="wrapper button-bar">
  <div class="button-form">
    <% if logged_in? %>
    <form action="/session" method="post">
      <button>
        Sign Out
      </button>
      <input type="hidden" name="_method" value="delete"/>
    </form>
    <% end %>
  </div>
</div>
